    "@types/mongoose": "^4.7.1",
    "@types/morgan": "^1.7.32",
    "@types/node": "^6.0.52",
    "@types/node-uuid": "0.0.28",
    "angular-cli": "^1.0.0-beta.24",
    "angular2-jwt": "^0.1.27",
    "auth0-lock": "^10.7.3",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "core-js": "^2.4.1",
    "cors": "^2.8.1",
    "express": "^4.14.0",
