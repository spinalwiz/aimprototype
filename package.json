{
  "name": "aimapp",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "node": "6.3.0"
  },
  "author": "Parkhouse",
  "description": "Aimtrain App",
  "angular-cli": {},
  "scripts": {
    "startHEROKU": "node ./src/server/app.js",
    "start": "concurrently --kill-others \"ng serve --proxy-config proxy.conf.json\" \"npm run _server:run\"",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test --watch=false",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "build": "ng build --prod --output-path=dist/client && npm run _server:build",
    "_server:run": "tsc -p ./src/server && concurrently \"tsc -w -p ./src/server\" \"nodemon dist/server/bin/www.js\" ",
    "_server:build": "tsc -p ./src/server",
    "postinstall--": "npm run build",
    "heroku-postbuild--": "ng build"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~2.3.1",
    "@angular/compiler": "~2.3.1",
    "@angular/compiler-cli": "~2.3.1",
